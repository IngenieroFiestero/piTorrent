doctype html
html
  head
    title= title
  body
    h3 #{idiom.menu}
    form(method="post")
      input(type="text" name="newTorrentLink" required)
      input(type="submit" value= "#{idiom.send}")
    h3 #{idiom.list}
    ul
      each vec in vector
        each por in porcent
          li #{vec}  --  #{por} %
    h3 #{idiom.config}
    form(method="post")
      select(name='idiom')
        option(value="en") English
        option(value="es") Espa√±ol
      input(type="submit" value= "#{idiom.send}")
